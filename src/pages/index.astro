---
// components
import IndexLayout from "../layouts/IndexLayout.astro";
import Nav from "../components/shared/Nav.astro";
import Footer from "../components/shared/Footer.astro";
import WhatsApp from "../components/shared/WhatsApp.astro";
import Card from "../components/index/PricingCard.astro";
import ClientCard from "../components/index/ClientCard.astro";

// images
import IntroImage from "../img/labstudio/pageImages/intro-side-icon.webp";
import WebDev from "../img/labstudio/pageImages/webdev-ico.webp";
import Branding from "../img/labstudio/pageImages/branding-ico.webp";
import VideoCamera from "../img/labstudio/pageImages/video-camera-3d.webp";
import WeMadeYourWeb from "../img/labstudio/pageImages/weMadeYourWeb.webp";
import Store from "../img/labstudio/pageImages/store-1.webp";
import GoogleBusiness from "../img/labstudio/pageImages/g-shadow.webp";
import WhatsappBusiness from "../img/labstudio/pageImages/w-shadow.webp";

// clients logos 
import AlferLogo from "../img/brands/brands-alfer.webp";
import ValleyLogo from "../img/brands/brands-valley-farms.webp";
import TonerLogo from "../img/brands/brands-toner.webp";
import TotalLogo from "../img/brands/brands-total.webp";
import SavingLogo from "../img/brands/brands-pharmacy.webp";
import CyberLogo from "../img/brands/brands-cyber.webp";
import CuciiLogo from "../img/brands/brands-cucii.webp";
import CustomBtn from "../components/shared/CustomBtn.astro";
---

<IndexLayout
	title="LABSTUDIO | Marketing Digital | Fotografía | Video | Desarrollo Web"
	ogTitle="LABSTUDIO | Marketing Digital | Fotografía | Video | Desarrollo Web"
	description="En Labstudiomedia, brindamos fotografía, video, desarrollo web y drones. Creatividad y enfoque en el cliente para el éxito de tu marca."
	ogDescription="En Labstudiomedia ofrecemos servicios de fotografía, producción de video, desarrollo web y videos aéreos con dron en Los Mochis, Sinaloa. Destaca con nuestra experiencia en medios digitales. Contáctanos hoy."
	keywords="Fotografía profesional, Servicios fotográficos en Los Mochis, Fotógrafo en Sinaloa, Sesiones fotográficas creativas, Fotografía de productos, Producción de video en Los Mochis, Videos corporativos, Edición de video profesional, Producción audiovisual en Sinaloa, Videos promocionales creativos, Diseño web personalizado, Desarrollo de sitios web en Los Mochis, Agencia de diseño web en Sinaloa, Desarrollo web responsive, Creación de tiendas en línea, Videos aéreos en Los Mochis, Filmación con dron en Sinaloa, Servicios de dron para eventos, Fotografía aérea profesional, Videos panorámicos con dron, Desarrollo de apps web en Los Mochis, Creación de aplicaciones web personalizadas, Desarrollo de software a medida, Aplicaciones web para negocios, Desarrollo de aplicaciones móviles web"
>
	<Nav/>
	<main>
		<section class="pt-4 pt-md-11">
			<div class="container">
				<div class="row align-items-center">
					<div class="intro-image-container col-12 col-md-5 col-lg-5 order-md-2">
						<img
							id="welcome-image"
							src={IntroImage.src}
							alt="Ilustracion de introducción en la portada"
							class="img-introduction img-fluid mb-2 mb-md-0"
						/>
					</div>
					<div class="col-12 col-md-7 col-lg-7 order-md-1">
						<h1 class="text-center text-md-start">
							Potenciamos tu presencia <br>digital transformando ideas <br>en contenidos digitales con <span
							class="text-dark-labs-gradient">desarrollo web</span>, <span class="text-dark-labs-gradient">video</span> y <span
							class="text-dark-labs-gradient">branding</span>
						</h1>
						<p class="lead text-center text-md-start text-muted mb-6">
							Quitate el miedo de aumentar tus ventas, por que es lo peor que puede pasar!
						</p>
						<div class="d-flex intro-btn-container">
							<CustomBtn
							href="https://wa.me/526681057964"
							blank={ true }
							isPrimary={ true }
							>
								Mas Información 
								<i class="fe fe-arrow-right d-none d-md-inline ms-3"></i>
							</CustomBtn>
							<CustomBtn
								href="https://www.tiktok.com/@labstudiomedia"
								blank={ true }
								isPrimary={ false }
							>
								Conócenos
							</CustomBtn>
						</div>
					</div>
				</div>
			</div>
			<div class="py-10 py-md-11">
				<div class="container">
					<div class="row">
						<div class="col-12 col-md-4 services-intro" data-aos="fade-up" data-aos-duration="600">
							<div class="service-card">
								<div class="icon-container mb-3">
									<img src={WebDev.src} alt="Ilustración sobre una pagina web"/>
								</div>
								<p class="card-intro-title text-dark-labs-gradient">
									Desarrollo Web
								</p>
								<p class="text-muted mb-0">
									Desarrollamos tu sitio web de tu negocio para que llegues a
									nuevos clientes.
								</p>
							</div>
						</div>
						<div class="col-12 col-md-4 services-intro" data-aos="fade-up" data-aos-duration="600" data-aos-delay="50">
							<di class="service-card">
								<div class="icon-container mb-3">
									<img src={Branding.src} alt="Ilustración sobre diseño"/>
								</div>
								<p class="card-intro-title text-dark-labs-gradient"> Branding </p>
								<p class="text-muted mb-6 mb-md-0">
									Diseñamos el futuro de tu negocio con estrategia y creatividad.
								</p>
							</di>
						</div>
						<div class="col-12 col-md-4 services-intro" data-aos="fade-up" data-aos-duration="600" data-aos-delay="100">
							<div class="service-card">
								<div class="icon-container mb-3">
									<img src={VideoCamera.src} alt="Ilustración de un cohete"/>
								</div>
								<p class="card-intro-title text-dark-labs-gradient">
									Producción Audiovisual
								</p>
								<p class="text-muted mb-6 mb-md-0">
									"Luces, Cámara, Acción": Potenciamos tu Mensaje con Videos
									Espectaculares.
								</p>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>

		<section class="pt-8 pt-md-11">
			<div class="container">
				<div
					class="row align-items-center justify-content-between flex-sm-column-reverse flex-column-reverse flex-md-row pb-8 pb-md-11">
					<div class="col-12 col-md-6 order-md-2">
						<h2>
							Creamos tu pagina web<br/>Una web para
							<span
								id="for-your-text"
								class="text-dark-labs-gradient"
								data-typed='{"strings": ["tu Empresa...", "tu Proyecto...", "tu Marca...", "ti..."]}'
							>
              </span>
						</h2>
						<p class="fs-lg text-muted mb-6">
							En un mundo digitalizado, tu presencia en línea es fundamental.
							Es por eso que en <span class="lab-font-color">LabStudio</span> Creamos
							sitios web impactantes y a tu medida que no solo destacan en Internet,
							sino que también ayudan a conectarte de manera efectiva con tus clientes,
							Creando una presencia digital más efectiva lo que se traduce en mas ventas
							para tu negocio.
						</p>

						<div class="row checks-container">
							<div class="col-12 col-lg-6">
								<div class="d-flex">
									<div data-aos="zoom-in" data-aos-duration="100" data-aos-easing="ease-in-out"
											 class="badge soft-lab-back-color lab-font-color badge-rounded-circle mt-1 me-4 check-boing">
										<i class="fe fe-check"></i>
									</div>
									<p class="lab-font-color">Creativo</p>
								</div>
								<div class="d-flex">
									<div data-aos="zoom-in" data-aos-duration="100" data-aos-easing="ease-in-out" data-aos-delay="50"
											 class="badge soft-lab-back-color lab-font-color badge-rounded-circle mt-1 me-4 check-boing">
										<i class="fe fe-check"></i>
									</div>
									<p class="lab-font-color mb-lg-0">Original</p>
								</div>
							</div>
							<div class="col-12 col-lg-6 mb-6 mb-md-0">
								<div class="d-flex">
                  <span data-aos="zoom-in" data-aos-duration="100" data-aos-easing="ease-in-out" data-aos-delay="100"
												class="badge soft-lab-back-color lab-font-color badge-rounded-circle mt-1 me-4 check-boing">
                    <i class="fe fe-check"></i>
                  </span>
									<p class="lab-font-color">Funcional</p>
								</div>
								<div class="d-flex">
									<div data-aos="zoom-in" data-aos-duration="100" data-aos-easing="ease-in-out" data-aos-delay="150"
											 class="badge soft-lab-back-color lab-font-color badge-rounded-circle me-1 me-4 check-boing">
										<i class="fe fe-check"></i>
									</div>
									<p class="lab-font-color mb-0">Efectivo</p>
								</div>
							</div>
						</div>
					</div>
					<div class="col-12 col-md-6 col-lg-5 order-md-1">
						<div class="web-s-control">
							<img src={WeMadeYourWeb.src} alt="Imagen de una búsqueda web"/>
						</div>
					</div>
				</div>
			</div>
		</section>

		<section class="pt-8 pt-md-11" id="web-design-scroll">
			<div class="container pb-8">
				<div class="row align-items-center">
					<div class="col-12 col-md-7 col-lg-6">
						<h2>
							Creamos tu<br/><span class="text-dark-labs-gradient"
						>Tienda en linea</span
						>
						</h2>

						<p class="fs-lg text-muted mb-6">
							Tu negocio puede llegar a clientes en todo el mundo, no solo localmente.
						</p>

						<div class="d-flex">
							<div id="eye-pages" class="icon icon-labs">
								<span class="fe fe-eye"></span>
							</div>
							<div class="ms-5">
								<h3 class="mb-1 h4">La primera impresión importa</h3>
								<p class="text-muted mb-6">
									Entendemos como funciona el mundo digital, es por eso que
									sabemos mejorar la experiencia de usuario con sitios web
									atractivos y responsivos.
								</p>
							</div>
						</div>
						<div class="d-flex">
							<div id="clock-pages" class="icon icon-labs">
								<span class="fe fe-clock"></span>
							</div>
							<div class="ms-5">
								<h3 class="mb-1 h4">Optimizamos tu pagina</h3>
								<p class="text-muted mb-6 mb-md-0 justify-text">
									Google favorece los sitios web con diseño responsivo, ya que ofrecen una mejor experiencia de usuario.
									Esto puede traducirse en una clasificación más alta en los resultados de búsqueda, lo que aumenta la
									visibilidad de tu negocio!
								</p>
							</div>
						</div>
					</div>
					<div class="col-12 col-md-5 col-lg-6">
						<div class="position-relative">
							<div class="img-skewed img-skewed-start">
								<img
									loading="lazy"
									src={Store.src}
									class="img-store-size screenshot img-fluid img-skewed-item"
									alt="Ilustración tienda en linea"
								/>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>

		<section class="pb-2 pt-10 d-flex align-items-center flex-column w-100">
			<h2 class="display-4 fw-bold pb-6 text-center">Sitios Web</h2>
			<div class="d-flex flex-wrap justify-content-center gap-6 w-100 align-items-start" data-aos="fade-up">
				<Card
					title="Básico"
					description="Tengo un negocio y quiero contar con presencia en línea, para generar más confianza con mis clientes y aumentar mis ventas!"
					whatsAppMessage="Hola LabStudio, estoy interesado en el paquete Básico para mi página web!!"
					price="8,499"
					image={null}
					features={[
						"Dominio Gratis por 1 año",
						"Hosting Gratis por 1 año",
						"Diseño Personalizado",
						"Certificado SSL",
						"Maps",
						"Botón WhatsApp",
						"Optimización Web & Móvil",
						"Soporte Básico"
					]}
				/>

				<Card
					title="Landing"
					description="Lo que todo negocio necesita, un escaparate para ayudar a tus clientes a conocerte un poco mas antes de realizar una compra"
					whatsAppMessage="Hola LabStudio, estoy interesado en el paquete Landing para mi página web!!"
					price="12,999"
					image={null}
					features={[
						"Dominio Gratis por 1 año",
						"Hosting Gratis por 1 año",
						"Diseño Personalizado",
						"Certificado SSL",
						"Maps",
						"Botón WhatsApp",
						"Optimización Web & Móvil",
						"Soporte Avanzado",
						"Links a tus Redes Sociales",
						"10 Cuentas de Correo",
						"Optimización SEO",
					]}
				/>

				<Card
					title="Deluxe"
					description="La pagina que tu Empresa se merece, de lujo, con la mejor experiencia de usuario para sorprender a tus clientes!"
					whatsAppMessage="Hola LabStudio, estoy interesado en el paquete VIP para mi página web!!"
					price="15,999"
					image={null}
					features={[
						"Dominio Gratis por 1 año",
						"Hosting Gratis por 1 año",
						"Diseño Personalizado",
						"Certificado SSL",
						"Maps",
						"Botón WhatsApp",
						"Optimización Web & Móvil",
						"Soporte Premium",
						"Links a tus Redes Sociales",
						"20 Cuentas de Correo",
						"Optimización SEO",
						"Blog",
						"Google Analytics",
						"Animaciones",
						"Integración de video",
					]}
				/>
			</div>
		</section>

		<section class="pb-2 pt-10 d-flex align-items-center flex-column w-100">
			<h2 class="display-4 fw-bold pb-6 text-center">Tiendas En Linea</h2>
			<div class="d-flex flex-wrap justify-content-center gap-6 w-100 align-items-start" data-aos="fade-up">
				<Card
					title="Mi Tienda en Linea"
					description="para los que quieren vender sus productos en todo el mundo!"
					whatsAppMessage="Hola LabStudio, estoy interesado en el paquete Mi Tienda para mi página tienda en linea!!"
					price="19,999"
					image={null}
					features={[
						"Dominio Personalizado",
						"Hosting Gratis por 1 año",
						"Diseño Personalizado",
						"Certificado SSL",
						"Optimización Web & Móvil",
						"Optimización SEO",
						"Avisos a Clientes por Email",
					]}
				/>
			</div>
		</section>

		<section class="pb-2 pt-10 d-flex align-items-center flex-column w-100">
			<div class="container">
				<h2 class="display-4 text-center fw-bold">Mas Soluciones Digitales</h2>
				<div class="col-12 pt-6 d-flex justify-content-center gap-6 flex-wrap" data-aos="fade-up">
					<Card
						title="WhatsApp Business"
						description="Convierte cada conversación en una oportunidad de ventas."
						whatsAppMessage="Hola Labstudio, estoy interesado en el servicio de WhatsApp Business!!"
						price=""
						image={WhatsappBusiness}
						features={[
							"Configuración de Plataforma",
							"Diseño de Catalogo",
							"Hasta 50 Respuestas Rápidas",
							"Etiquetas",
							"Anuncios desde WhatsApp",
							"Implementación",
						]}
					/>

					<Card
						title="Google My Business"
						description="Quiero que mis clientes puedan encontrar facilmente mi Negocio."
						whatsAppMessage="Hola Labstudio, estoy interesado en el servicio de Google My Business!!"
						price=""
						image={GoogleBusiness}
						features={[
							"Verificación de tu Negocio",
							"Configuración del Perfil",
							"Configuración de Tienda",
							"Anuncios en Google",
							"Google Analytics",
							"Posicionamiento SEO",
						]}
					/>
				</div>
			</div>
		</section>

		<!-- BRANDS -->
		<div class="py-5 py-md-10 w-100">
			<div class="container">
				<h2 class="brands-h2 fw-bold text-center mb-2">Nuestros clientes</h2>
				<p class="text-muted text-center pb-1">Estamos orgullosos de trabajar con algunas de las mejores empresas de la
					industria</p>
				<div class="d-flex flex-wrap gap-2 align-items-center justify-content-center">

					<ClientCard imageSrc={AlferLogo} delay={0} width={ 1347 } height={ 403 }/>
					<ClientCard imageSrc={ValleyLogo} delay={50} width={ 500 } height={ 500 }/>
					<ClientCard imageSrc={TonerLogo} delay={100} width={ 1080 } height={ 1080 }/>
					<ClientCard imageSrc={TotalLogo} delay={150} width={ 1630 } height={ 419 }/>
					<ClientCard imageSrc={SavingLogo} delay={200} width={ 300 } height={ 157 }/>
					<ClientCard imageSrc={CyberLogo} delay={250} width={ 1500 } height={ 1500 }/>
					<ClientCard imageSrc={CuciiLogo} delay={300} width={ 500 } height={ 500 }/>

				</div>
			</div>
		</div>

		<div class="py-5 py-md-10">
			<div class="container">
				<h2 class="brands-h2 fw-bold text-center mb-2">¿Nos quieres contactar?</h2>
				<p class="text-muted text-center pb-0 big-subtext">Contáctanos y hablemos de cómo podemos llevar tu proyecto al siguiente nivel.</p>
			</div>
			<div class="container d-flex flex-column align-items-center pt-md-0 gap-4">

				<div class="d-flex flex-column justify-content-center align-items-center">
					<div class="w-100 d-flex justify-content-center align-items-center gap-2 flex-wrap">


						<CustomBtn
							href="https://wa.me/526681057964"
							isPrimary={ true }
							extraClasses="contact-links gap-1 text-decoration-none py-2 d-flex align-items-center"
						>
							<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="#ffffff" class="bi bi-whatsapp" viewBox="0 0 16 16" > <path d="M13.601 2.326A7.85 7.85 0 0 0 7.994 0C3.627 0 .068 3.558.064 7.926c0 1.399.366 2.76 1.057 3.965L0 16l4.204-1.102a7.9 7.9 0 0 0 3.79.965h.004c4.368 0 7.926-3.558 7.93-7.93A7.9 7.9 0 0 0 13.6 2.326zM7.994 14.521a6.6 6.6 0 0 1-3.356-.92l-.24-.144-2.494.654.666-2.433-.156-.251a6.56 6.56 0 0 1-1.007-3.505c0-3.626 2.957-6.584 6.591-6.584a6.56 6.56 0 0 1 4.66 1.931 6.56 6.56 0 0 1 1.928 4.66c-.004 3.639-2.961 6.592-6.592 6.592m3.615-4.934c-.197-.099-1.17-.578-1.353-.646-.182-.065-.315-.099-.445.099-.133.197-.513.646-.627.775-.114.133-.232.148-.43.05-.197-.1-.836-.308-1.592-.985-.59-.525-.985-1.175-1.103-1.372-.114-.198-.011-.304.088-.403.087-.088.197-.232.296-.346.1-.114.133-.198.198-.33.065-.134.034-.248-.015-.347-.05-.099-.445-1.076-.612-1.47-.16-.389-.323-.335-.445-.34-.114-.007-.247-.007-.38-.007a.73.73 0 0 0-.529.247c-.182.198-.691.677-.691 1.654s.71 1.916.81 2.049c.098.133 1.394 2.132 3.383 2.992.47.205.84.326 1.129.418.475.152.904.129 1.246.08.38-.058 1.171-.48 1.338-.943.164-.464.164-.86.114-.943-.049-.084-.182-.133-.38-.232" ></path> </svg>
							<p
								class="m-0"
							>(668) 105 79 64</p>
						</CustomBtn>

						<CustomBtn
							href="mailto:contacto@labstudiomedia.com"
							isPrimary={ false }
							extraClasses="contact-links gap-1 text-decoration-none py-2 d-flex align-items-center"
						>
							<svg xmlns="http://www.w3.org/2000/svg" fill="#8f8f8f" width="20" height="20" class="bi bi-envelope-fill" viewBox="0 0 16 16"> <path d="M.05 3.555A2 2 0 0 1 2 2h12a2 2 0 0 1 1.95 1.555L8 8.414zM0 4.697v7.104l5.803-3.558zM6.761 8.83l-6.57 4.027A2 2 0 0 0 2 14h12a2 2 0 0 0 1.808-1.144l-6.57-4.027L8 9.586zm3.436-.586L16 11.801V4.697z"/> </svg>
							<p class="m-0">contacto@labstudiomedia.com</p>
						</CustomBtn>

					</div>
				</div>

			</div>
		</div>

		<div class="py-5 py-md-10">
			<div class="container">
				<h2 class="brands-h2 fw-bold text-center mb-2">¿Todavía no te convences?</h2>
				<p class="text-muted text-center pb-0 big-subtext">Conoce mas de nuestro trabajo</p>
			</div>
			<div class="d-flex flex-column align-items-center justify-content-center">
				<div class="d-flex gap-2">

					<a
						href="https://www.tiktok.com/@labstudiomedia"
						target="_blank"
						class="text-decoration-none contact-icon"
						aria-label="TikTok Link"
					>
						<svg
							xmlns="http://www.w3.org/2000/svg"
							width="16"
							height="16"
							fill="currentColor"
							class="bi bi-tiktok"
							viewBox="0 0 16 16"
						>
							<path
								d="M9 0h1.98c.144.715.54 1.617 1.235 2.512C12.895 3.389 13.797 4 15 4v2c-1.753 0-3.07-.814-4-1.829V11a5 5 0 1 1-5-5v2a3 3 0 1 0 3 3z"
							></path>
						</svg>
					</a>

					<a
						href="https://www.instagram.com/labstudiomedia/"
						target="_blank"
						class="text-decoration-none contact-icon"
						aria-label="Instagram Link"
					>
						<svg
							xmlns="http://www.w3.org/2000/svg"
							width="16"
							height="16"
							fill="currentColor"
							class="bi bi-instagram"
							viewBox="0 0 16 16"
